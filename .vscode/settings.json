{
    "cSpell.words": [
        "aiomqtt",
        "dataframe",
        "iframe",
        "matplot",
        "matplotlib",
        "mosquitto",
        "mqtt",
        "oocana",
        "ovmlayer",
        "paho",
        "plotly",
        "pypi",
        "pyright",
        "pytest",
        "SIGALRM",
        "userdata",
        "venv"
    ],
    "python.analysis.typeCheckingMode": "basic",
    "yaml.schemas": {
        "https://static.oomol.com/json-schemas/Block.schema.json": "block.oo.yaml",
        "https://static.oomol.com/json-schemas/Flow.schema.json": "flow.oo.yaml",
    },
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.testing.pytestArgs": [
        "oocana",
        "executor"
    ],
    "git.branchProtection": ["main"],
    "git.branchProtectionPrompt": "alwaysCommitToNewBranch",
    "git.branchRandomName.enable": true
}